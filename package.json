{
  "name": "envdotyml",
  "version": "0.0.0",
  "description": "Load environment variables from a .env.yml file",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/envdotyml",
  "bugs": "https://github.com/vinsonchuong/envdotyml/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/envdotyml",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {
    "js-yaml": "^4.0.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "semantic-release": "^17.4.2",
    "xo": "^0.38.2"
  },
  "ava": {
    "verbose": true
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/no-anonymous-default-export": "off"
    },
    "ignores": [
      "set.js"
    ]
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
